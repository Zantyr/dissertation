[{
  "name":"RIASEC example test",
  "datafile":"static/quiz.json",
  "logfile":"static/qviz.tsv",
  "accuracy":false,
  "additional":{},
  "initialize":[
                    ["log","Fit classifiers there"],
                    ["log","Fitting on random sample..."],
                    ["$nbrand",["fit","naiveBayes","Strength",["load","static/0.csv"]]],
                    ["log","Fitting on same preference sample..."],
                    ["$nbpref",["fit","naiveBayes","Strength",["load","static/1.csv"]]],
                    ["log","Fitting on same age profile sample..."],
                    ["$nbage",["fit","naiveBayes","Strength",["load","static/2.csv"]]],
                    ["log","Fitting on equal gender countries..."],
                    ["$nbgend",["fit","naiveBayes","Strength",["load","static/3.csv"]]]
               ],
  "reports":[
                 ["print","Klasyfikator losowy: {}",["predict","$nbrand","df"]],
                 ["print","Klasyfikator preferencyjny: {}",["predict","$nbpref","df"]],
                 ["print","Klasyfikator wiekowy: {}",["predict","$nbage","df"]],
                 ["print","Klasyfikator plciowy: {}",["predict","$nbgend","df"]]
            ],
  "batch": [
                ["safePrint","<h3>Cluster the data and count the clusers</h3>"]
           ]
},
{
  "name":"RIASEC reduced set",
  "datafile":"static/reduced.json",
  "logfile":"static/reduced.tsv",
  "accuracy":true,
  "additional":{
    "age":"textInteger",
    "country":"text",
    "gender":"textInteger"
  },
  "initialize":[
                    ["log","Fitting on feature selected..."],
                    ["$nbfeat",["fit","naiveBayes","Strength",["load","static/4.csv"]]]
                ],
  "reports":[
                 ["print","Klasyfikator zredukowany: {}",["predict","$nbgend","df"]]
            ],
  "batch": [
                ["safePrint","<h3>Cluster the data and count the clusters</h3>"]
           ]
}]
